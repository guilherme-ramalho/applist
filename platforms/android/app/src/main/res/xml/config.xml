<?xml version='1.0' encoding='utf-8'?>
<widget id="com.atlanteti.inventario" version="1.0.0" xmlns="http://res/iconwandroidts" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Atlante Invent√°rio</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="desenvolvimento@atlanteti.com" href="http://atlanteti.com">
        Atlante TI
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/icon-36-ldpi.png" />
    <icon density="mdpi" src="res/icon/android/icon-48-mdpi.png" />
    <icon density="hdpi" src="res/icon/android/icon-72-hdpi.png" />
    <icon density="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
